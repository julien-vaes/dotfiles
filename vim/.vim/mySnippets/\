##########################
## Optimisation problem ##
##########################

snippet min "minimum optimmisation problem" w
\begin{mini!}|l|[2]
  {${2:variables}}
  {${3:objective function}, \label{eq:$1_objective}}
  {\label{op:${1:label}_problem}}
  {${7}}
  \addConstraint{${5}}{${6}. \label{eq:$1_constraint}}
\end{mini!}%
\vspace{-5mm}
endsnippet

snippet max "maximum optimmisation problem" w
\begin{maxi!}|l|[2]
  {${2:variables}}
  {${3:objective function}, \label{eq:$1_objective}}
  {\label{op:${1:label}_problem}}
  {${7}}
  \addConstraint{${5}}{${6}. \label{eq:$1_constraint}}
\end{maxi!}%
\vspace{-5mm}
endsnippet

