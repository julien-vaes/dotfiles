##########
## Sets ##
##########

# No parentheses set
snippet bset ".,...,." w
\bset{${1}}{${2}}${0}
endsnippet

# No parentheses set starting at 1
snippet bseto ".,...,." w
\bseto{${1}}${0}
endsnippet

# Discrete set
snippet dset "{.,...,.}" w
\dset{${1}}{${2}}${0}
endsnippet

snippet dseto "{1,...,.}" w
\dseto{${1}}${0}
endsnippet

# Discrete set in range manner
snippet range "|[.,...,.]|" w
\range{${1}}{${2}}${0}
endsnippet

# Discrete set with start to 1 in range manner
snippet rangeo "|[1,...,.]|" w
\rangeo{${1}}${0}
endsnippet

# Continuous set
snippet cset "[.,.]" w
\cset{${1}}{${2}}${0}
endsnippet

# Set condition
snippet setc "{.|.}" w
\setc{${1}}{${2}}${0}
endsnippet

###########
## Norms ##
###########

# Norm
snippet norm "||.||_." i
\norm{${1:norm}}{${2}}${0}
endsnippet

# Abs
snippet abs "|.|" i
\abs{${1}}${0}
endsnippet

#################################
## Probability and expectation ##
#################################

# Probability
snippet proba "P[.]" i
\proba{${1}}${0}
endsnippet

# Probability with distribution
snippet probadist "P_.[.]" i
\probadist{${1:dist}}{${2}}${0}
endsnippet

# Conditional probability
snippet cproba "P[.|.]" i
\cproba{${1}}{${2:condition}}${0}
endsnippet

# Conditional probability with distribution
snippet cprobadist "P_.[.|.]" i
\cprobadist{${1:dist}}{${2}}{${3:condition}}${0}
endsnippet

# Expectation
snippet expe "E[.]" w
\expe{${1}}${0}
endsnippet

# Expectation
snippet expem "E^.[.]" w
\expem{${1:measure}}{${2}}${0}
endsnippet

# Conditional expectation
snippet cexpe "E[.|.]" w
\cexpe{${1}}{${2:condition}}${0}
endsnippet

# Conditional expectation with specified probability measure
snippet cexpe "E[.|.]" w
\cexpe{${1}}{${2:condition}}${0}
endsnippet

# VaR
snippet varr "VaR[.]" w
\varr{${1:quantile}}{${2}}${0}
endsnippet

# Conditional VaR
snippet cvarr "VaR[.|.]" w
\cvarr{${1:quantile}}{${2}}{${3:condition}}${0}
endsnippet

# CVaR
snippet cvar "CVaR[.]" w
\cvar{${1:quantile}}{${2}}${0}
endsnippet

# Conditional CVaR
snippet ccvar "CVaR[.|.]" w
\ccvar{${1:quantile}}{${2}}{${3:condition}}${0}
endsnippet

# Variance
snippet vari "V[.]" w
\vari{${1}}${0}
endsnippet

# Conditional variance
snippet cvari "V[.|.]" w
\cvari{${1}}{${2:condition}}${0}
endsnippet

